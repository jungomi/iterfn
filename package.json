{
  "name": "iterator-adapters",
  "version": "0.1.0",
  "description": "Functions which take an iterator and return another iterator",
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "repository": {},
  "author": "Michael Jungo <michaeljungo92@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:browser": "BABEL_ENV=browser NODE_ENV=production webpack --progress",
    "docs": "documentation build -f html -o docs src/index.js --github --name $npm_package_name --project-version $npm_package_version",
    "lint": "eslint .",
    "test": "BABEL_ENV=commonjs ava --verbose",
    "clean": "rimraf lib dist docs"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.14.1",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1"
  }
}
